package cdac;

public class SinglyLinkedList {
	//node inner class
	static class Node{
		int data;
		Node next;
		Node(int d){
			data=d;
			next=null;
		}
	}
	Node head;// head of list
	public void append(int new_data) {
		Node new_node=new Node(new_data);
		if(head==null) {
			head=new_node;
			return;
			
		}
	}
	

}
